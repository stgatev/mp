{
    "name": "express-template",
    "version": "1.0.0",
    "description": "Express Template",
    "main": "dist/server.js",
    "repository": "git@github.com:stgatev/express-template.git",
    "author": "Stan Gatev <stgatev@yahoo.com>",
    "license": "SEE LICENSE IN license.txt",
    "scripts": {
        "clean": "rimraf dist",
        "build": "tsc",
        "prebuild": "npm run clean",
        "postbuild": "cpx src/**/*.{json,js} dist",
        "start": "node dist/server.js",
        "prestart": "npm run build",
        "test": "mocha --reporter spec --require ts-node/register --exit tests/**/*.test.ts",
        "pretest": "npm run build",
        "wstart": "concurrently \"tsc -w\" \"cpx src/**/*.{json,js} dist --watch\" \"node dist/server.js\"",
        "prewstart": "npm run build"
    },
    "dependencies": {
        "body-parser": "^1.18.2",
        "chai": "^4.1.2",
        "chai-http": "^3.0.0",
        "config": "^1.28.1",
        "cpx": "^1.5.0",
        "express": "^4.16.2",
        "jsonpath": "^1.0.0",
        "jsonwebtoken": "^8.1.0",
        "mocha": "^4.0.1",
        "mongoose": "^4.13.5",
        "morgan": "^1.9.0",
        "passport": "^0.4.0",
        "passport-google-oauth20": "^1.0.0",
        "path": "^0.12.7",
        "swaggerize-express": "^4.0.5",
        "swaggerize-ui": "^1.0.1",
        "ts-node": "^3.3.0",
        "typescript": "^2.6.1",
        "uuid": "^3.1.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.16.8",
        "@types/chai": "^4.0.4",
        "@types/chai-http": "^3.0.3",
        "@types/config": "^0.0.33",
        "@types/express": "^4.0.39",
        "@types/jsonwebtoken": "^7.2.3",
        "@types/mocha": "^2.2.44",
        "@types/mongoose": "^4.7.28",
        "@types/morgan": "^1.7.35",
        "@types/node": "^8.0.50",
        "@types/passport": "^0.4.0",
        "@types/uuid": "^3.4.3",
        "concurrently": "^3.5.0",
        "rimraf": "^2.6.2"
    }
}
